{ {% load jetpack_extras %}{% load base_helpers %}
	"message": "Library \"{{ cap.name }} \" in version {{ version.fullname }} added as dependency to \"{{ item.fullname }}\"",
	"dependency": {
		"dependency_link_html": "{% with version as capversion %}{% escape_template "_dependency_list_item.html" %}{% endwith %}",
		"dependency_textarea_html": "{% with version as cap_version %}{% escape_template "_dependency_content_textarea.html" %}{% endwith %}",
		"slug": "{{ cap.slug }}",
		"name": "{{ cap.name }}",
		"creator": "{{ cap.creator }}",
		"type": "capability",
		"version_create_url": "{{ cap.get_version_create_url }}",
		"version": {
			"slug": "{{ cap.slug }}",
			"remove_url": "{{ dependency_remove_url }}",
			"author": "{{ version.author }}",
			"name": "{{ version.name }}",
			"counter": {{ version.counter }},
			"description": "{{ version.description|escapejs }}",
			"content": "{{ version.content|escapejs }}",
			"is_base": {{ capversion.is_base|yesno:"true,false" }},
			"edit_url": "{{ version.get_absolute_url }}",
			"update_url": "{{ version.get_update_url }}",
			"switch_content_id": "dependency_{{ cap.slug }}",
			"content_el": {
				"element": "dependency_content_{{ cap.slug }}"
			}
		}
	}
}
